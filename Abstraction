using System;
public abstract class Cow
{
    public abstract void milk();
}
class Buffelo : Cow
{
    public override void milk()
    {
        Console.WriteLine("Give milk ");
    }


}
public abstract class BabyBuffelo
{
    public abstract void milks();
}
class BabyCow : BabyBuffelo
{
    public override void milks()
    {
        Console.WriteLine("give milk ");
    }
}

public class Testmilk
{
    public static void Main(string[] args)
    {
        Buffelo c = new Buffelo();
        c.milk();
        BabyCow b = new BabyCow();
        b.milks();


    }
}

